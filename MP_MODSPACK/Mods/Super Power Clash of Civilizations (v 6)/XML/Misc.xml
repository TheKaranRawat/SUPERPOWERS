<GameData>
	<!--**************************Can Produce Process at Game Start**************************-->
	<Processes>
		<Update>
			<Set TechPrereq="NULL"/>
			<Where Type="PROCESS_WEALTH"/>
		</Update>
		<Update>
			<Set TechPrereq="NULL"/>
			<Where Type="PROCESS_RESEARCH"/>
		</Update>

	</Processes>


	<!--**********************************City Min Yield**********************-->

	<Yields>
		<Update>
			<Set MinCity="2"/>
			<Where Type="YIELD_PRODUCTION"/>
		</Update>
	</Yields>

	<!--**********************************Worlds Size Mods**********************-->
	<Worlds>
		<Update>
			<Set NumCitiesUnhappinessPercent="100"/>
			<Where Type="WORLDSIZE_LARGE"/>
		</Update>
		<Update>
			<Set NumCitiesUnhappinessPercent="100"/>
			<Where Type="WORLDSIZE_HUGE"/>
		</Update>
		
		<Update>
			<Set NumCitiesPolicyCostMod="0"/>
			<Where Type="WORLDSIZE_DUEL"/>
		</Update>
		<Update>
			<Set NumCitiesTechCostMod="5"/>
			<Where Type="WORLDSIZE_DUEL"/>
		</Update>
		<Update>
			<Set NumCitiesPolicyCostMod="0"/>
			<Where Type="WORLDSIZE_TINY"/>
		</Update>
		<Update>
			<Set NumCitiesTechCostMod="5"/>
			<Where Type="WORLDSIZE_TINY"/>
		</Update>
		<Update>
			<Set NumCitiesPolicyCostMod="0"/>
			<Where Type="WORLDSIZE_SMALL"/>
		</Update>
		<Update>
			<Set NumCitiesTechCostMod="4"/>
			<Where Type="WORLDSIZE_SMALL"/>
		</Update>
		<Update>
			<Set NumCitiesPolicyCostMod="0"/>
			<Where Type="WORLDSIZE_STANDARD"/>
		</Update>
		<Update>
			<Set NumCitiesTechCostMod="4"/>
			<Where Type="WORLDSIZE_STANDARD"/>
		</Update>
		<Update>
			<Set EstimatedNumCities="50"/>
			<Where Type="WORLDSIZE_STANDARD"/>
		</Update>

		<Update>
			<Set NumCitiesPolicyCostMod="0"/>
			<Where Type="WORLDSIZE_LARGE"/>
		</Update>
		<Update>
			<Set NumCitiesTechCostMod="3"/>
			<Where Type="WORLDSIZE_LARGE"/>
		</Update>
		<Update>
			<Set EstimatedNumCities="85"/>
			<Where Type="WORLDSIZE_LARGE"/>
		</Update>
		<Update>
			<Set ResearchPercent="125"/>
			<Where Type="WORLDSIZE_LARGE"/>
		</Update>

		<Update>
			<Set NumCitiesPolicyCostMod="0"/>
			<Where Type="WORLDSIZE_HUGE"/>
		</Update>
		<Update>
			<Set NumCitiesTechCostMod="2"/>
			<Where Type="WORLDSIZE_HUGE"/>
		</Update>
		<Update>
			<Set MaxActiveReligions="8"/>
			<Where Type="WORLDSIZE_HUGE"/>
		</Update>
		<Update>
			<Set EstimatedNumCities="110"/>
			<Where Type="WORLDSIZE_HUGE"/>
		</Update>
		<Update>
			<Set ResearchPercent="150"/>
			<Where Type="WORLDSIZE_HUGE"/>
		</Update>
	</Worlds>


