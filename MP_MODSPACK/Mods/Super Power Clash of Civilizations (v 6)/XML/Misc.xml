	<!--**********************************Unit Mission ICON Changes******************************-->
	<Missions>
		<Update>
			<Set IconIndex="6" IconAtlas="SPUINT_ACTION_ATLAS"/>
			<Where Type="MISSION_CULTURE_BOMB"/>
		</Update>
	</Missions>



	<!--**********************************Cities Names Correction******************************-->
	<Language_en_US>
		<Replace Tag="TXT_KEY_CITY_NAME_SHANDONG">
			<Text>Jinan</Text>
		</Replace>
		<Replace Tag="TXT_KEY_CITY_NAME_LIAONING">
			<Text>Shenyang</Text>
		</Replace>
		<Replace Tag="TXT_KEY_CITY_NAME_HUBEI">
			<Text>Wuhan</Text>
		</Replace>
		<Replace Tag="TXT_KEY_CITY_NAME_XIKANG">
			<Text>Dali</Text>
		</Replace>
	</Language_en_US>

	
	<!--**********************************Mission Priority******************************-->
	<Missions>
		<Update>
			<Where Type="MISSION_AIRPATROL"/>
			<Set OrderPriority="199"/>
		</Update>
	</Missions>



	<!--**********************************AI Flavors Tweak******************************-->


